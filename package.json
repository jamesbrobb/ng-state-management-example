{
  "name": "state-management",
  "version": "0.0.0",
  "scripts": {
    "start-elf": "ng serve location-weather --configuration elf-dev --port=4201",
    "start-ngrx": "ng serve location-weather --configuration ngrx-dev --port=4202",
    "start-ngxs": "ng serve location-weather --configuration ngxs-dev --port=4203",
    "build-libs": "ng build shared && ng build elf && ng build ngrx && ng build ngxs",
    "build-elf-lib": "ng build shared && ng build elf",
    "build-ngrx-lib": "ng build shared && ng build ngrx",
    "build-ngxs-lib": "ng build shared && ng build ngxs",
    "build-elf-app": "ng build location-weather --configuration elf-prod",
    "build-ngrx-app": "ng build location-weather --configuration ngrx-prod",
    "build-ngxs-app": "ng build location-weather --configuration ngxs-prod",
    "build-all": "npm run build-libs && npm run build-elf-app && npm run build-ngrx-app && npm run build-ngxs-app",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.0",
    "@angular/common": "^16.1.0",
    "@angular/compiler": "^16.1.0",
    "@angular/core": "^16.1.0",
    "@angular/forms": "^16.1.0",
    "@angular/material": "^16.1.0",
    "@angular/platform-browser": "^16.1.0",
    "@angular/platform-browser-dynamic": "^16.1.0",
    "@angular/router": "^16.1.0",
    "@ngneat/effects": "^2.1.1",
    "@ngneat/effects-ng": "^3.1.2",
    "@ngneat/elf": "^2.3.1",
    "@ngneat/elf-cli-ng": "^1.0.0",
    "@ngneat/elf-devtools": "^1.2.1",
    "@ngneat/elf-entities": "^4.4.4",
    "@ngneat/elf-pagination": "^1.1.0",
    "@ngneat/elf-persist-state": "^1.1.6",
    "@ngneat/elf-requests": "^1.7.0",
    "@ngneat/elf-state-history": "^1.3.0",
    "@ngrx/component-store": "^16.0.1",
    "@ngrx/effects": "^16.0.1",
    "@ngrx/entity": "^16.0.1",
    "@ngrx/router-store": "^16.0.1",
    "@ngrx/store": "^16.0.1",
    "@ngrx/store-devtools": "^16.0.1",
    "@ngxs/router-plugin": "^3.8.0",
    "@ngxs/store": "^3.8.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^16.0.0",
    "@angular-devkit/build-angular": "^16.1.0",
    "@angular/cli": "^16.1.0",
    "@angular/compiler-cli": "^16.1.0",
    "@ngxs/devtools-plugin": "^3.8.0",
    "@types/jasmine": "~4.3.0",
    "@types/leaflet": "^1.9.3",
    "@types/node": "^20.10.4",
    "dotenv-webpack": "^8.0.1",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^16.1.0",
    "typescript": "~4.9.4"
  }
